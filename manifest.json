{
  "manifest_version": 3,
  "name": "Hoof it - CRXJS React Vite Example",
  "version": "1.0.0",
  "content_scripts": [
    {
      "js": ["src/scripts/github-replace-merge.ts"],
      "matches": ["https://github.com/*"],
      "run_at": "document_end"
    },
    {
      "js": ["src/scripts/cycling-news-localStorage.ts"],
      "matches": ["https://www.cyclingnews.com/*"],
      "run_at": "document_end"
    }
  ]
}
